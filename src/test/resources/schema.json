{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "Car": {
      "$id": "/properties/Car",
      "type": "array",
      "items": {
        "$id": "/properties/Car/items",
        "type": "object",
        "properties": {
          "make": {
            "$id": "/properties/Car/items/properties/make",
            "type": "string",
            "title": "The Make Schema ",
            "default": "",
            "examples": [
              "Audi"
            ]
          },
          "model": {
            "$id": "/properties/Car/items/properties/model",
            "type": "string",
            "title": "The Model Schema ",
            "default": "",
            "examples": [
              "A7"
            ]
          },
          "vin": {
            "$id": "/properties/Car/items/properties/vin",
            "type": "string",
            "title": "The Vin Schema ",
            "default": "",
            "examples": [
              "09AGHY64352JITEG98K"
            ]
          },
          "metadata": {
            "$id": "/properties/Car/items/properties/metadata",
            "type": "array",
            "items": {
              "$id": "/properties/Car/items/properties/metadata/items",
              "type": "object",
              "properties": {
                "Color": {
                  "$id": "/properties/Car/items/properties/metadata/items/properties/Color",
                  "type": "string",
                  "title": "The Color Schema ",
                  "default": "",
                  "examples": [
                    "Black"
                  ]
                },
                "Notes": {
                  "$id": "/properties/Car/items/properties/metadata/items/properties/Notes",
                  "type": "string",
                  "title": "The Notes Schema ",
                  "default": "",
                  "examples": [
                    "Scratches on the front side of the car"
                  ]
                }
              }
            }
          },
          "perdayrent": {
            "$id": "/properties/Car/items/properties/perdayrent",
            "type": "array",
            "items": {
              "$id": "/properties/Car/items/properties/perdayrent/items",
              "type": "object",
              "properties": {
                "Price": {
                  "$id": "/properties/Car/items/properties/perdayrent/items/properties/Price",
                  "type": "integer",
                  "title": "The Price Schema ",
                  "default": 0,
                  "examples": [
                    140
                  ]
                },
                "Discount": {
                  "$id": "/properties/Car/items/properties/perdayrent/items/properties/Discount",
                  "type": "integer",
                  "title": "The Discount Schema ",
                  "default": 0,
                  "examples": [
                    15
                  ]
                }
              }
            }
          },
          "metrics": {
            "$id": "/properties/Car/items/properties/metrics",
            "type": "array",
            "items": {
              "$id": "/properties/Car/items/properties/metrics/items",
              "type": "object",
              "properties": {
                "yoymaintenancecost": {
                  "$id": "/properties/Car/items/properties/metrics/items/properties/yoymaintenancecost",
                  "type": "number",
                  "title": "The Yoymaintenancecost Schema ",
                  "default": 0,
                  "examples": [
                    2190.760009765625
                  ]
                },
                "depreciation": {
                  "$id": "/properties/Car/items/properties/metrics/items/properties/depreciation",
                  "type": "number",
                  "title": "The Depreciation Schema ",
                  "default": 0,
                  "examples": [
                    2256.429931640625
                  ]
                },
                "rentalcount": {
                  "$id": "/properties/Car/items/properties/metrics/items/properties/rentalcount",
                  "type": "array",
                  "items": {
                    "$id": "/properties/Car/items/properties/metrics/items/properties/rentalcount/items",
                    "type": "object",
                    "properties": {
                      "lastweek": {
                        "$id": "/properties/Car/items/properties/metrics/items/properties/rentalcount/items/properties/lastweek",
                        "type": "integer",
                        "title": "The Lastweek Schema ",
                        "default": 0
                      },
                      "yeartodate": {
                        "$id": "/properties/Car/items/properties/metrics/items/properties/rentalcount/items/properties/yeartodate",
                        "type": "integer",
                        "title": "The Yeartodate Schema ",
                        "default": 0,
                        "examples": [
                          221
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}

